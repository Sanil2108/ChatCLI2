<resources>
    <string name="app_name">ChatCLI</string>
    <string name="title_activity_dostheme">DOSThemeActivity</string>

    <string name="DOSOpeningMessage">SOG Chat CLI - WinDOS [Version 0.5.14393]\n
(c) 2017 com.sanilk.chatCLI </string>

    <string-array name="help">
        <item>\@conn [name]</item>
        <item>Connects to the user with the name you entered</item>

        <item>\@disconn</item>
        <item>Disconnects you from the current user</item>

        <item>\@check</item>
        <item>Checks for new messages</item>

        <item>\@list</item>
        <item>Shows the list of currently connected users</item>

        <item>\@help_advanced</item>
        <item>Provides advanced help</item>

        <item>\@logout</item>
        <item>Logs out the current user</item>

    </string-array>

    <string-array name="help_advanced">
        <item>\@dev [message]</item>
        <item>Send me a message. I\'m most probably bored anyway</item>

        <item>\@ping</item>
        <item>pings the server and displays the time taken for the response</item>
    </string-array>
</resources>
